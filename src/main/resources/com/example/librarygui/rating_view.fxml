<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="200.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarygui.controllers.RateView">
   <children>
      <HBox prefHeight="15.0" prefWidth="41.0">
         <children>
            <FontAwesomeIconView glyphName="STAR" fill="yellow"/>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" fx:id="ratingText" />
         </children>
      </HBox>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" fx:id="commentText" />
   </children>
</VBox>
